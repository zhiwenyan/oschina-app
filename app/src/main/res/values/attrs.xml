<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SquareLayout">
        <attr name="oscAccordTo" >
            <enum name="width" value="1"/>
            <enum name="height" value="2"/>

        </attr>
    </declare-styleable>

    <declare-styleable name="TweetPicturesLayout">
        <attr name="verticalSpace" />
        <attr name="horizontalSpace" />
        <attr name="column" format="integer" />
        <attr name="maxPictureSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ToggleButton">
        <attr name="toggle_border_width" format="dimension">
        </attr>
        <attr name="offBorderColor" format="reference|color">
        </attr>
        <attr name="offColor" format="reference|color">
        </attr>
        <attr name="onColor" format="reference|color">
        </attr>
        <attr name="spotColor" format="reference|color">
        </attr>
    </declare-styleable>
</resources>